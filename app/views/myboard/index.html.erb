<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/header' %>
  </head>

  <div id="body">

          <div class="container">

                <br/><br/>

            <h1>My Board</h1>
            <% if session[:user_id] %>
              <%= render 'user_myboard' %>
            <%end%>
            <% if session[:organization_id] %>
              <%= render 'org_myboard' %>
           
            <% end %>
            

          </div>


  </div>
  <div id="footer"><%= render 'layouts/footer' %></div>
</html>